{
	"compilerOptions": {
		"noImplicitAny": false,
		"preserveConstEnums": true,
		"downlevelIteration": true,
		"sourceMap": false,
        "outDir": "dist",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "declaration": false,
        "lib": [
            "dom",
            "es2020"
        ],
        "baseUrl": "src/main/ts",
        "paths": {
            ".": [""]
        },
        "target": "es2020",
        "jsx": "react"
	},
    "include": [
        "src/**.ts",
        "src/**/**.ts",
        "src/**.tsx",
        "src/**/**.tsx"
    ]
}

