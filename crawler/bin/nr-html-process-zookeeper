function writeAnyNumberBE(n, buffer, offset, bytes) {
    let bigintN = BigInt(n);
    for (let i = 0; i < bytes; ++i) {
        const byte = (bigintN & BIGINT_MASK);
        buffer[offset + bytes - 1 - i] = Number(byte);
        bigintN >>= BIGINT_EIGHT;
    }
}
