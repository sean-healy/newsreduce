}
function wordsFromNode(node, hitType) {
    const content = contentFromNode(node, hitType);
    const lc = content.toLowerCase();
    const normalised = lc.replace(/[–-]/g, "");
    const words = normalised.match(/[^ \t\s!"'£$4€%^&*();:@~#\n®.,<>?/\[\]\\{}“”‘’]+/g);
