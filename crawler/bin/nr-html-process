function getLinks(window) {
    const parent = new ResourceURL["ResourceURL"](window.location.toString());
    const aHrefs = window.document.getElementsByTagName(AHREF);
    const links = [];
    for (let position = 0; position < aHrefs.length; ++position) {
        const a = aHrefs.item(position);
        if (a.href) {
            const parts = a.href.split(HASH, 2);
            const url = parts[0];
            const hash = parts.length > 1 ? parts[1] : "";
            let child;
            try {
                child = new ResourceURL["ResourceURL"](url);
                const link = new ResourceLink["ResourceLink"]({ parent, child });
                if (hash)
                    links.push(new ResourceLinkHash["ResourceLinkHash"]({ link, hash: new ResourceHash["ResourceHash"](hash) }));
                else
                    links.push(link);
            }
            catch (parseError) {
                Object(logging["a" /* log */])("invalid url", parseError);
            }
        }
    }
    return links;
}
